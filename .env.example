# Configurações do Laboratório de Automação N8N
# Copie este arquivo para .env e ajuste as configurações conforme necessário

# ==============================================
# CONFIGURAÇÕES GERAIS
# ==============================================
COMPOSE_PROJECT_NAME=automation-lab
TIMEZONE=America/Sao_Paulo

# ==============================================
# CONFIGURAÇÕES DO N8N
# ==============================================
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=admin123
N8N_HOST=localhost
N8N_PORT=5678
N8N_PROTOCOL=http
WEBHOOK_URL=http://localhost:5678/
N8N_LOG_LEVEL=info
N8N_ENCRYPTION_KEY=your-encryption-key-here-change-this-in-production
N8N_USER_MANAGEMENT_DISABLED=false
N8N_COMMUNITY_PACKAGES_ENABLED=true

# ==============================================
# CONFIGURAÇÕES DO POSTGRESQL
# ==============================================
POSTGRES_DB=postgres
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres123
POSTGRES_PORT=5432

# Bancos de dados adicionais (separados por vírgula)
POSTGRES_MULTIPLE_DATABASES=n8n,automation_db,test_db

# Usuários adicionais (formato: usuario:senha,usuario2:senha2)
POSTGRES_MULTIPLE_USERS=n8n_user:n8n_password,auto_user:auto_password,test_user:test_password

# ==============================================
# CONFIGURAÇÕES DO SQL SERVER
# ==============================================
MSSQL_SA_PASSWORD=SqlServer123!
MSSQL_PID=Developer
MSSQL_TCP_PORT=1433
MSSQL_AGENT_ENABLED=true

# ==============================================
# CONFIGURAÇÕES DO REDIS
# ==============================================
REDIS_PASSWORD=redis123
REDIS_PORT=6379

# ==============================================
# CONFIGURAÇÕES DO PGADMIN
# ==============================================
PGADMIN_DEFAULT_EMAIL=admin@automation.local
PGADMIN_DEFAULT_PASSWORD=admin123
PGADMIN_PORT=8080

# ==============================================
# CONFIGURAÇÕES DO ADMINER
# ==============================================
ADMINER_PORT=8081

# ==============================================
# CONFIGURAÇÕES DO NGINX
# ==============================================
NGINX_HTTP_PORT=80
NGINX_HTTPS_PORT=443

# ==============================================
# CONFIGURAÇÕES DE REDE
# ==============================================
NETWORK_SUBNET=172.20.0.0/16

# ==============================================
# CONFIGURAÇÕES DE VOLUMES
# ==============================================
DATA_PATH=./data
LOGS_PATH=./logs
CONFIG_PATH=./config
BACKUP_PATH=./backups

# ==============================================
# CONFIGURAÇÕES DE DESENVOLVIMENTO
# ==============================================
# Definir como 'true' para modo de desenvolvimento
DEV_MODE=true

# Definir como 'true' para habilitar debug
DEBUG_MODE=false

# Definir como 'true' para recriar containers sempre
FORCE_RECREATE=false